<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>KNX.net</PackageId>
    <Title>KNX.net</Title>
    <Authors>life.emotions</Authors>
    <Description>KNX.net provides a KNX API for .NET, providing Tunneling and Routing connection modes.</Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageLicenseUrl>https://github.com/lifeemotions/knx.net/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/lifeemotions/knx.net</PackageProjectUrl>
    <Copyright>Copyright 2018 (c) Life Emotions. All rights reserved.</Copyright>
    <PackageTags>knx .net c#</PackageTags>
    <PackageReleaseNotes></PackageReleaseNotes>
    <Version>3.0.0</Version>
  </PropertyGroup>
  
  <PropertyGroup>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0-*" PrivateAssets="All"/>
  </ItemGroup>

</Project>
